    "# Import required modules\n",
    "import numpy as np\n",
    "from __future__ import print_function\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib import style\n",
    "from pprint import pprint\n",
    "from IPython.display import display\n",
    "import seaborn as sns; sns.set(font_scale=1.2)\n",
    "\n",
    "import pandas as pd\n",
    "from sklearn import datasets, linear_model\n",
    "from sklearn.model_selection import train_test_split\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "# Load the data into variable 'df'\n",
    "df = pd.read_csv('https://raw.githubusercontent.com/cdmorgan103/7331DataMiningNoShow/master/data/CleanedData.csv')\n",
    "\n",
    "#new variables\n",
    "df['age_range'] = pd.cut(df.Age,[0,16,65,1e6],3,labels=['child','adult','senior']) \n",
    "distinct_pID = df['PatientId'].nunique()\n",
    "cat_var = df[['Gender','age_range','Neighbourhood','Scholarship','Hypertension','Diabetes','Alcoholism','Handicap','SMSReceived','ScheduledDOW','AppointmentDOW','NoShow']]\n",
    "con_var = df[['ScheduledDay','ScheduledTime','AppointmentDay']]\n",
    "df = df.replace({'NoShow': {'Yes': True, 'No': False}})\n",
    "\n",
    "# Convert date-time variables to the correct type using the C-style fmt codes\n",
    "df.ScheduledDay = pd.to_datetime(df.ScheduledDay, format=\"%Y-%m-%d\")\n",
    "df.ScheduledTime = pd.to_datetime(df.ScheduledTime, format=\"%H:%M:%S\")\n",
    "df.AppointmentDay = pd.to_datetime(df.AppointmentDay, format=\"%Y-%m-%d\")\n",
    "\n",
    "#Remove attributes not usefull\n",
    "del df['PatientId'] #not sure if we need to remove, might be helpful with identifying multiple appointments by Patient \n",
    "del df['Unnamed: 0']\n",
    "\n",
    "#Computed discrete features agains now with the newest values\n",
    "df['age_range'] = pd.cut(df.Age,[0,16,24,65,1e6],4,labels=[0,1,2,3]) # this creates a new variable\n",
    "df['ScheduledDOW'] = df['ScheduledDOW'].astype('category')\n",
    "df['AppointmentDOW'] = df['AppointmentDOW'].astype('category')\n",
    "\n",
    "# Get an overview of the raw data\n",
    "df.info(null_counts=True)"
   ]
